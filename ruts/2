    $(document).ready(function(){
        //funcion idiomas
        $(".dropdown-toggle").on("click", function(){
            if($(".dropdown-menu").is(":visible")){
                $(".caret").addClass('giro');
                $(".caret").removeClass('giro');
            }
            else{
                $(".caret").addClass('giro');
            }
        });
    });
