    var p = jQuery.noConflict();
    p(function() {
        p('.more').live("click", function(e) {
            e.preventDefault();
            var ID = p(this).attr("id");
			var nid = parseInt(ID) + 1;
            if (ID) {
               lang = '';
               // code = document.getElementById("code").value;
                inicio = '';
                final = '';
                meco_code = '';
                p.ajax({
                    type: "POST",
                    url: "/rc/company/mas_products.asp",
                    data: "lastmsg=" + ID + "&meco_code=" + 300375156,
                    cache: false,
                    success: function(html) {
                        p("#accordion-2").append(html);
						//p(".more").attr("id", nid);	
						p("#viewmore" + ID).remove()
                    }
                });
            } else {
                p(".morebox").html('The End');
            }
            return false;
        });
    });
	
	
	
